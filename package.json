{
  "name": "code-notes",
  "version": "0.0.4",
  "description": "Code Notes",
  "scripts": {
    "dev-front": "webpack-dev-server -r dotenv/config --config ./webpack/configuration/development.config.js",
    "prod-front": "webpack -r dotenv/config --config ./webpack/configuration/production.config.js",
    "serve-front-end": "yarn prod-front && node server/serve-front-end.js",
    "dev-back": "nodemon -r dotenv/config server/index.js",
    "prod-back": "",
    "server": "yarn prod-front && node -r dotenv/config server/index.js",
    "e2e-dev-webpack": "start-server-and-test serve-front-end http-get://localhost:5000 cy-run",
    "e2e-dev": "start-server-and-test serve-front-end :5000 cy-open",
    "e2e-prod": "start-server-and-test serve-front-end :5000 cy-run",
    "cy-run": "cypress run",
    "cy-open": "cypress open",
    "unit-test": "jest",
    "lint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,css,yml}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/code-notes.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/code-notes/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/code-notes#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "babel-eslint": "^10.0.1",
    "codemirror": "^5.45.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-static-gzip": "^1.1.3",
    "js-cookie": "^2.2.0",
    "node-sass": "^4.11.0",
    "ramda": "^0.26.1",
    "redux-form": "^8.2.0",
    "url-loader": "^2.0.0",
    "webpack-pwa-manifest": "^4.0.0",
    "winston": "3.0.0",
    "winston-mongodb": "3.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "bcrypt": "^3.0.4",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^1.0.1",
    "cypress": "^3.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express-async-errors": "^3.1.1",
    "file-loader": "^2.0.0",
    "helmet": "^3.15.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.0",
    "lint-staged": "^8.1.6",
    "live-serve": "^0.0.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "mongoose": "^5.4.16",
    "morgan": "^1.9.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hot-loader": "^4.9.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "serve": "^10.1.1",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
