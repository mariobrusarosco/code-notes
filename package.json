{
  "name": "code-notes",
  "version": "0.0.1",
  "description": "Code-Notes",
  "private": true,
  "scripts": {
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint --fix src/**/*.{css,scss}",
    "unit-test": "yarn eslint && yarn stylelint && jest",
    "unit-test-ww": "walter-white",
    "e2e-dev": "start-server-and-test server-prod :5000 cy-open",
    "e2e-prod": "start-server-and-test server-prod :5000 cy-run",
    "cy-run": "cypress run",
    "cy-open": "cypress open",
    "start": "export NODE_ENV=local && yarn app-init && webpack-dev-server --config ./webpack/configuration/development.config.babel.js",
    "build-dev": "export NODE_ENV=development && yarn app-init && webpack --config ./webpack/configuration/development.config.babel.js",
    "build-prod": "export NODE_ENV=production && yarn app-init && webpack --config ./webpack/configuration/production.config.babel.js",
    "server-dev": "yarn build-dev && node server/index.js",
    "server-prod": "yarn build-prod && node server/index.js",
    "app-init": "babel-node -r dotenv/config scripts/create-config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint"
      ],
      "*.{ts,tsx,js,css,scss,yml}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/eng-zap-challenge.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/eng-zap-challenge/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/eng-zap-challenge#readme",
